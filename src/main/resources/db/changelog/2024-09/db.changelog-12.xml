<?xml version="1.0" encoding="UTF-8"?>  
<databaseChangeLog
   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">

<changeSet id="13" author="Brieuc">

    <createTable tableName="entry_free_food">
        <column name="id" type="VARBINARY(36)">
            <constraints primaryKey="true" primaryKeyName="PK_ENTRY_FREE_FOOD" nullable="false"/>
        </column>
        <column name="version" type="long"/>
        <column name="title" type="varchar(255)"/>
        <column name="description"  type="varchar(255)"/>
        <column name="date" type="date"/>

        <column name="kcal" type="integer"/>
        <column name="food_type" type="varchar(50)"/>

        <column name="modelid" type="VARBINARY(36)">
            <constraints foreignKeyName="fk_model_free_food" references="model_free(id)" nullable="false"/>
        </column>
    </createTable>
</changeSet>

</databaseChangeLog>